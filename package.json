{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20 <=22.19.0"
  },
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client",
    "build:client": "cd client && vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "node dist/server/node-build.mjs",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "db:setup": "tsx server/scripts/setup-database.ts",
    "db:migrate": "tsx server/database/migrations/001_initial_schema.ts",
    "db:migrate-analytics": "tsx server/database/migrations/002_analytics_enhancement.ts",
    "db:migrate-payments": "tsx server/database/migrations/003_payment_system.ts",
    "db:seed": "tsx server/database/seeders/001_seed_menu_data.ts",
    "db:seed-admin": "tsx server/database/seeders/002_seed_admin_users.ts",
    "db:seed-analytics": "tsx server/database/seeders/003_analytics_sample_data.ts",
    "db:seed-offers": "tsx server/database/seeders/004_seed_offers.ts",
    "db:full-setup": "npm run db:migrate && npm run db:migrate-analytics && npm run db:migrate-payments && npm run db:seed && npm run db:seed-admin && npm run db:seed-analytics && npm run db:seed-offers",
    "vercel-build": "npm run build:client",
    "vercel-dev": "npm run dev",
    "db:check": "node -e \"console.log('âœ… Using Neon Database - no local setup needed'); process.exit(0)\""
  },
